{
    "name": "dwoodard/a2a",
    "description": "Laravel package for Google's Agent-to-Agent (A2A) protocol integration.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Woodard",
            "email": "dustin@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": ">=9.0",
        "illuminate/mail": ">=9.0",
        "illuminate/database": ">=9.0",
        "illuminate/notifications": ">=9.0",
        "illuminate/bus": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "Dwoodard\\A2A\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dwoodard\\A2A\\LaravelAgentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}